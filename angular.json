{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "get2hire": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/get2hire",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap-grid.css",
              "src/assets/css/icons.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/style.css",
              "src/assets/css/responsive.css",
              "src/assets/css/chosen.css",
              "src/assets/css/colors/colors.css",
              "src/assets/css/bootstrap.css",
              "node_modules/font-awesome/css/font-awesome.css"
            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/js/modernizr.js",
              "src/assets/js/script.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/parallax.js",
              "src/assets/js/select-chosen.js",
              "src/assets/js/counter.js",
              "src/assets/js/circle-progress.min.js",
              "src/assets/js/isotop.js",
              "src/assets/js/bootstrap-datepicker.js",
              "src/assets/js/tag.js",
              "src/assets/js/select-chosen.js",
              "src/assets/js/rslider.js",
              "src/assets/js/prashant-autocomplete.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "get2hire:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "get2hire:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "get2hire:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "get2hire:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "get2hire:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "get2hire"
}
